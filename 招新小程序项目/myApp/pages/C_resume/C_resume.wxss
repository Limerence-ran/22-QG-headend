.box{
  background-color: #fff;

  /* margin-top: 20rpx; */
  padding: 25rpx 20rpx;
  margin: 20rpx 30rpx;
  border: 1px solid rgba(236, 236, 236, 1);
box-shadow: 0px 4px 16px 0px rgba(133, 133, 133, 0.25);
border-radius: 16px;
  /* box-shadow: 0 2px 6px rgb(63, 57, 57); */
}
.nav{
  margin-bottom: 15rpx;
  display: flex;
  align-items: center;
}
.dot {
  background-color: #5b08c3;
  margin-right: 12rpx;
  border-radius: 50rpx;
  padding: 10rpx;
  box-shadow: 0 0 5rpx 5rpx #a0cfff;
}

.title {
  font-size: 30rpx;
  font-weight: bold;
}
.row {
  display: flex;
  flex-direction: row;
  padding: 25rpx 0;
  margin: 20rpx 30rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #f1f1f1;
  justify-content: space-between;
}

.end {
  border: none;
  padding-bottom: 10rpx;
}
.right {
  color: rgb(110, 110, 110);
}

.right input {
  text-align: right;
  width: 100%;
  text-overflow:unset;
  overflow:auto;
}

.required {
  color: red;
  margin-right: 5px;
}
.submitBtn{
  margin: 0 30rpx;
  /* background: -webkit-gradient(linear, left top, left bottom, from(#eff7ff), color-stop(1, #4f41ce), to(#eff7ff)); */
background-color: #5b08c3;
color: white;
}

.triangle{
  font-size: 10px;
  

  border-radius: 5px; /* 添加圆角 */
}
