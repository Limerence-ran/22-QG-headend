<view class="box">
  <van-notice-bar left-icon="volume-o" scrollable="{{ true }}" background="#5b08c3" color="#fff" class="custom-class" text="在代码阅读过程中人们说脏话的频率是衡量代码质量的唯一标准。" />

  <view class="title">
    <view>面试组别：<text>{{groupName}}</text></view>
    <view>面试导师：<text>蒋卓然、温惠兰</text></view>
    <view>面试地点：<text>{{place}}</text></view>
  </view>
  <view class="sign">
    <button hover-class="custom-class" bind:tap="signUp">签到</button>
  </view>
  <view class="position">
    <view>
      <van-icon name="location-o" color="#5b08c3" size="30" />
      <text>{{address}}</text>
    </view>
    <view bind:tap="location">重新定位</view>
  </view>

  <view class="message">
    <van-steps steps="{{ steps }}" active="{{ active }}" direction="vertical" active-color="#5b08c3" />
  </view>

</view>
<view class="cancel" bind:tap="Cancel">
  <button class="btn"> 取消预约</button>
  <view class="line"></view>
</view>
<buttombar></buttombar>