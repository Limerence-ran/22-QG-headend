  text-align: center;
  margin-top: 0.4em;
  background: linear-gradient(120deg, rgba(0,194,255,1) 0%, rgba(0,86,255,1) 100%);
  background-clip: border-box;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
 }
 
 .card .info > span {
  font-weight: bold;
  font-size: 1.2em;
 }
 

