	<!-- 团队文字 -->
	<view style="width: 100%; position: absolute;">
		<image src="data:image/png;base64," mode="" style="width: 100%; image-rendering:crisp-edges;" />
	</view>
</view>
<!-- 格子主体 -->
<!-- 工作室内容 -->
<view class="studio_container">
	<view class="title_font">
		工作室介绍
	</view>
	<van-grid column-num="4" clickable="{{true}}" border="{{false}}">
		<!-- 指导老师 -->
		<van-grid-item use-slot>
			<image style="width: 28px;height: 28px; " src="../../../utils/svg/teacher.svg" />
			<view>
				<view class="grid_font_main">导师</view>
			</view>
		</van-grid-item>
		<!-- 团队特色 -->
		<van-grid-item use-slot>
			<image style="width: 28px;height: 28px; " src="../../../utils/svg/feature.svg" />
			<view>
				<view class="grid_font_main">特色</view>
			</view>
		</van-grid-item>
		<!-- 团队荣誉 -->
		<van-grid-item use-slot>
			<image style="width: 28px;height: 28px; " src="../../../utils/svg/honor.svg" />
			<view>
				<view class="grid_font_main">荣誉</view>
			</view>
		</van-grid-item>
		<!-- 团队成员 -->
		<van-grid-item use-slot>
			<image style="width: 28px;height: 28px; " src="../../../utils/svg/member.svg" />
			<view>
				<view class="grid_font_main">成员</view>
			</view>
		</van-grid-item>
	</van-grid>
</view>
<!-- 各小组内容 -->
<view class="group_container">
	<view class="title_font">
		小组介绍
	</view>
	<van-grid column-num="4" clickable="{{true}}" border="{{false}}">
		<!-- 前端组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/web_icon.svg" />
			<view>
				<view class="grid_font">前端</view>
			</view>
		</van-grid-item>
		<!-- 后台组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/sql_icon.svg" />
			<view>
				<view class="grid_font">后台</view>
			</view>
		</van-grid-item>
		<!-- 移动组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/mobile_icon.svg" />
			<view>
				<view class="grid_font">移动</view>
			</view>
		</van-grid-item>
		<!-- 人工智能组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/ai_icon.svg" />
			<view>
				<view class="grid_font">人工智能</view>
			</view>
		</van-grid-item>
		<!-- 嵌入式组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/embedded_icon.svg" />
			<view>
				<view class="grid_font">嵌入式</view>
			</view>
		</van-grid-item>
		<!-- 图形组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/graph_icon.svg" />
			<view>
				<view class="grid_font">图形</view>
			</view>
		</van-grid-item>
		<!-- 设计组 -->
		<van-grid-item use-slot>
			<image style="width: 20px;height: 20px; " src="../../../utils/svg/ui_icon.svg" />
			<view>
				<view class="grid_font">设计</view>
			</view>
		</van-grid-item>
	</van-grid>
</view>




