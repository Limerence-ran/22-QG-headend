<view class="main">
  <view class="right">
    <view class="title">
      <image src="{{avatarUrl}}" class="head" animation="{{shakeAnimation}}"></image>
      <text>前端组</text>
    </view>
    <view class="content">
      <view class="box">
        <view class="box_title">
          <image src="/utils/photos/info.svg" mode="aspectFit" class="icon" />
          个人信息
        </view>
        <view class="box_text">
          <view class="text_info">
            <view class="text_info_block">
              <view>姓名</view>
              <view><text>{{userInfo.name}}</text></view>
            </view>
            <view class="text_info_block">
              <view>学号</view>
              <view><text>{{userInfo.studentId}}</text></view>
            </view>
            <view class="text_info_block">
              <view>专业</view>
              <view><text>{{userInfo.major}}</text></view>
            </view>
          </view>
          <view class="text_info">
            <view class="text_info_block">
              <view>年龄</view>
              <view><text>{{userInfo.age}}</text></view>
            </view>
            <view class="text_info_block">
              <view>学院</view>
              <view><text>{{userInfo.college}}</text></view>
            </view>
            <view class="text_info_block">
              <view>电话</view>
              <view><text>{{userInfo.phone}}</text></view>
            </view>
          </view>
        </view>
      </view>
      <view class="box">
        <view class="box_title">
          <image src="/utils/photos/score.svg" mode="aspectFit" class="icon" />
          成绩情况
        </view>
        <view class="box_text">
          <view class="text_info">
            <view class="text_info_block">
              <view>挂科情况</view>
              <view><text>{{flunk==1?'是':'无'}}</text></view>
            </view>
            <view class="text_info_block">
              <view>英语成绩</view>
              <view><text>{{userInfo.english}}</text></view>
            </view>
            <view class="text_info_block">
              <view>C实验成绩</view>
              <view><text>{{userInfo.cexperiment}}</text></view>
            </view>
          </view>
          <view class="text_info">
            <view class="text_info_block">
              <view>绩点排名</view>
              <view><text>{{userInfo.gpa}}</text></view>
            </view>
            <view class="text_info_block">
              <view>C理论成绩</view>
              <view><text>90</text></view>
            </view>
          </view>
        </view>
      </view>
      <view class="box lastBox">
        <view class="box_title">
          <image src="/utils/photos/social.svg" mode="aspectFit" class="icon" />
          其他
        </view>
        <view class="box_text">
          <view class="text_info text_last">
            <view class="text_info_block">
              <view>社团情况</view>
              <view><text>{{userInfo.organization}}</text></view>
            </view>
            <view class="text_info_block">
              <view>座右铭</view>
              <view><text>{{userInfo.motto}}</text></view>
            </view>
          </view>
        </view>
      </view>
      <view class="empty">
        <buttombar></buttombar>
      </view>
    </view>
  </view>
</view>