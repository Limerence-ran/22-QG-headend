/* components/titlebar/titlebar.wxss */
.tab{
  width: 100%;
  background-color: white;
  position:sticky;
  top: 0;
  display: flex;
  align-items: flex-end;
  z-index: 999;
  padding-bottom: 10rpx;
}
.tab .utils{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items:flex-end;
  flex: 0.1;
}

.tab .logo{
  flex: 0.8;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tab .logo .titleimage{
  width: 110px;
  height: 30px;
}