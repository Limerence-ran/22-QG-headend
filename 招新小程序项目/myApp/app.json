{
  "pages": [
    "pages/index/index"
  ],
  "subpackages": [
    {
      "root": "packageA",
      "name": "pack1",
      "pages":[
        "pages/C_bookInterview/C_bookInterview"
      ]
    },{
      "root": "packageB",
      "name": "pack2",
      "pages": [
        "pages/home/home",
        "pages/C_queue/C_queue",
        "pages/C_score/C_score",
        "pages/C_resume/C_resume",
