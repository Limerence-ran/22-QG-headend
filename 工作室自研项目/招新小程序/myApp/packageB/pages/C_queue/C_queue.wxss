.sign {
  height: 300rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;

}

.sign button {
  height: 250rpx;
  width: 250rpx;
  padding: 20rpx;
  border-radius: 50%;
  background-color: #5b08c3;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: rgb(212, 211, 211);
  font-size: small;
  box-shadow: 0 0 0 #5b08c3;
  opacity: .8;
  animation: pulse 3s infinite;
}

.sign button text:nth-child(2) {
  font-size: 70rpx;
  line-height: 70rpx;
  color: #fff;
}

@keyframes pulse {
  0% {
    box-shadow: 0 0 0 #5b08c3;
  }

  70% {
    box-shadow: 0 0 60rpx rgba(0, 0, 255, 0);
  }

  100% {
    box-shadow: 0 0 0 rgba(0, 0, 255, 0);
  }
}


.update {
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around
}


@keyframes emit {
  0% {}

  100% {
    border-width: 1px;
    opacity: 0;
    transform: scale(1.3);
  }
}

.buttombar {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
}

/*动画样式*/
.eye-lid {
  background-color: rgb(0, 0, 0);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 9px 0 2px rgba(0, 0, 0, 0.2);
  width: 150px;
  height: 150px;
  scale: 0.8;
}

.eye {
  background-color: #fffcf4;
  border-radius: 50%;
  transform: translate3d(0, 0, 0) rotate(90deg);
  width: 120px;
  height: 120px;
  animation: eye 5s infinite;
}

@keyframes eye {

  12%,
  25% {
    width: 100px;
    height: 110px;
  }

  37%,
  50% {
    width: 60px;
    height: 130px;
  }

  63%,
  75% {
    width: 100px;
    height: 103px;
  }

  87% {
    width: 100px;
    height: 100px;
  }
}

.cornea {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: rgb(0, 0, 0);
  border-radius: 50%;
  width: 50px;
  height: 50px;
  animation: eye-color 5s infinite;
}

@keyframes eye-color {

  63%,
  75% {
    background-color: rgb(137, 1, 1);
  }
}

.white-pupil {
  position: absolute;
  top: 70%;
  left: 10%;
  border-radius: 50%;
  background-color: #fffcf4;
  width: 20px;
  height: 20px;
}

